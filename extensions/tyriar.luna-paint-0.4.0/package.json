	},
	"bugs": {
		"url": "https://github.com/lunapaint/vscode-luna-paint/issues"
	},
	"galleryBanner": {
		"theme": "light",
		"color": "#00ACC2"
	},
	"categories": [
		"Other",
		"Visualization"
